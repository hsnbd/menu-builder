{
    "name": "hsnbd/menu-builder",
    "description": "Flexible menu builder for laravel.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Baker Hasan",
            "email": "hasanbd666@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "ext-json": "*",
        "laravel/framework": "^8.12"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5@dev"
    },
    "autoload": {
        "psr-4": {
            "Hsnbd\\MenuBuilder\\": "/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hsnbd\\MenuBuilder\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hsnbd\\MenuBuilder\\MenuBuilderServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": false
}
